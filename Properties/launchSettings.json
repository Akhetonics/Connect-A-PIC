{
  "profiles": {
    "Debug": {
      "commandName": "Executable",
      "executablePath": "%Godot%",
      "commandLineArgs": "--path . --verbose",
      "workingDirectory": "."
    },
    "Debug GD Tests": {
      "commandName": "Executable",
      "executablePath": "%Godot%",
      "commandLineArgs": "--path . --run-tests --quit-on-finish --verbose",
      "workingDirectory": ".",
      "environmentVariables": {
        "DO_NOT_RUN_TUTORIAL": "0"
      },
      "nativeDebugging": false
    }
  }
}